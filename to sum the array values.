package com.java;
 
   public class Arrayprogram4{
	   
	   int sum;
	   
	   public static void main(String []args){
		  
		   int arr[]={4,6,8,2,10};
		   
		   int sum = 0;

	        for (int num : arr ) {
	        	
	         sum += num;

		   System.out.println("Sum Of Values In The Array :"+sum);
		   
	   }  
    }

  }
