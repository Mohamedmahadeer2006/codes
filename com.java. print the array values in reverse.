package com.java;

    public class Arrayprogram5{
    	
    	 public static void main(String[]args){
    		 
    	        int[] numbers = {5, 10, 15, 20, 25};
    	        
    	        System.out.println("Array in reverse order:");
    	        
    	       
    	        	for (int i = numbers.length-1 ; i >= 0; i--) {
    	     
    	            System.out.print(numbers[i] + " ");

    }
    	        	
    	        	
  }
    }    	 
    
